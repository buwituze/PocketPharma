//tsconfig
{
   "compilerOptions": {
     "lib": ["esnext"],
     "target": "esnext",
     "module": "commonjs",
     "moduleResolution": "node",
     "outDir": "./build",
     "rootDir": "./src",
     "emitDecoratorMetadata": true,
     "experimentalDecorators": true,
     "sourceMap": true,
     "strict": true,
     "esModuleInterop": true   // Add this line
   },
   "include": ["src/**/*.ts"],
   "exclude": ["node_modules", "build"]
 }
 